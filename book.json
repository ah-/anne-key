{
    "structure": {
        "summary": "SUMMARY.md"
    },
    "plugins" : [
      "edit-link",
      "forkmegithub",
      "hints",
      "terminal",
      "toolbar"
    ],
    "pluginsConfig": {
      "edit-link": {
        "base": "https://github.com/ah-/anne-key/edit/master/docs/src",
        "label": "Suggest an edit"
      },
      "forkmegithub": {
        "color": "red",
        "url": "https://github.com/ah-/anne-key"
      },
      "page-toc": {
        "selector": ".markdown-section h1, .markdown-section h2"
      },
      "terminal": {
        "copyButtons": true,
        "fade": false,
        "style": "flat"
      },
      "toolbar": {
        "buttons": [
          {
            "label": "Anne Pro",
            "icon": "fa fa-github",
            "url": "https://github.com/ah-/anne-key"
          }
        ]
      }
    },
    "root": "./docs/src/"
}
